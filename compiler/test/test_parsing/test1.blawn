class Person(name)
    @name = name
    @function hello()
        text = "Hello! I'm "
        text.append(self.name)
        print(text)
        return
    @function set_name(name)
        self.name = name
        return

p = Person("John")
p.hello()
p.set_name("Johhhn")
p.hello()
p.name = "I'm renamed."
p.hello()
