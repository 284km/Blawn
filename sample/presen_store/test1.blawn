class Person(name)
    @name = name
    @function hello()
        text = "Hello! I'm "
        text.append(self.name)
        print(text)
        return

john = Person("john")

function rename(person)
    bobif = Person("Bob")
    c = bobif
    person <- bobif
    return c

b = rename(john)
print(b.name)
john.hello()